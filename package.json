{
  "name": "@paywaz/sdk",
  "version": "0.1.0",
  "description": "Paywaz JavaScript SDK",
  "keywords": ["paywaz", "payments", "sdk", "api", "webhooks"],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Paywaz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellopaywaz/paywaz-sdk-js.git"
  },
  "homepage": "https://github.com/hellopaywaz/paywaz-sdk-js#readme",
  "bugs": {
    "url": "https://github.com/hellopaywaz/paywaz-sdk-js/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "smoke": "node ./scripts/smoke.mjs",
    "prepack": "npm run build",
    "test": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "@types/node": "^20.11.30",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2"
  }
}
