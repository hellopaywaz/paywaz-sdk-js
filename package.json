{
  "name": "@paywaz/sdk",
  "version": "0.1.0",
  "description": "Paywaz JavaScript SDK",
  "keywords": ["paywaz", "payments", "sdk", "api", "webhooks"],
  "license": "MIT",
  "author": "Paywaz",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellopaywaz/paywaz-sdk-js.git"
  },
  "homepage": "https://github.com/hellopaywaz/paywaz-sdk-js#readme",
  "bugs": {
    "url": "https://github.com/hellopaywaz/paywaz-sdk-js/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "node ./scripts/clean.mjs",
    "build": "node ./scripts/build.mjs",
    "smoke": "node ./scripts/smoke.mjs",
    "prepack": "npm run build",
    "test": "node ./scripts/test.mjs",
    "lint": "node ./scripts/lint.mjs",
    "typecheck": "node ./scripts/typecheck.mjs",
    "generate:sdk": "node ./scripts/generate-sdk.mjs",
    "docs": "typedoc"
  },
  "devDependencies": {
    "typedoc": "file:./tools/typedoc",
    "typescript": "5.9.3"
  }
}
