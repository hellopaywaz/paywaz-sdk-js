{
  "name": "@paywaz/sdk",
  "version": "0.1.0",
  "description": "Paywaz JavaScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "generate:sdk": "ts-node scripts/generate-sdk.ts",
    "docs": "typedoc",
    "test": "node -e \"console.log('ok')\"",
    "prepublishOnly": "npm run build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish --provenance",
    "release:beta": "npm run build && changeset publish --tag beta"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  }
}
